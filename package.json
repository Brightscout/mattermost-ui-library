{
	"name": "@brightscout/mattermost-ui-library",
	"version": "2.3.4",
	"author": {
		"name": "Brightscout, Inc."
	},
	"main": "build/index.js",
	"files": [
		"build"
	],
	"scripts": {
		"build": "rm -rf build && tsc && npm run copy-assets && npm run copy-dts && npm run build:scss && tsc-alias",
		"build:storybook": "build-storybook",
		"chromatic": "npx chromatic --exit-once-uploaded --project-token=$npm_config_projectToken",
		"copy-assets": "copyfiles -u 1 \"src/assets/**/*.svg\" build",
		"copy-dts": "copyfiles -u 2 \"src/**/*.d.ts\" -e \"src/types/**/*.d.ts\" -e \"src/**/react-app-env.d.ts\" build/components",
		"build:scss": "copyfiles -u 1 \"src/**/*.scss\" build",
		"generate-icon-map": "node scripts/generateIconMap.js",
		"lint": "tsc --noEmit && eslint src/**/*.ts{,x} --cache",
		"lint:fix": "eslint src/**/*.ts{,x} --fix --cache",
		"prepublishOnly": "npm run build",
		"storybook": "start-storybook -p 6006"
	},
	"devDependencies": {
		"@babel/core": "7.16.7",
		"@storybook/addon-actions": "6.5.12",
		"@storybook/addon-docs": "6.5.12",
		"@storybook/addon-essentials": "6.5.12",
		"@storybook/addon-interactions": "6.5.12",
		"@storybook/addon-links": "6.5.12",
		"@storybook/builder-webpack4": "6.5.12",
		"@storybook/manager-webpack4": "6.5.12",
		"@storybook/preset-create-react-app": "3.2.0",
		"@storybook/react": "6.5.12",
		"@storybook/testing-library": "0.0.13",
		"@svgr/webpack": "5.5.0",
		"@testing-library/jest-dom": "5.3.0",
		"@testing-library/react": "10.0.2",
		"@testing-library/user-event": "10.0.1",
		"@types/enzyme": "3.10.12",
		"@types/enzyme-adapter-react-16": "1.0.6",
		"@types/jest": "26.0.24",
		"@types/node": "16.3.1",
		"@types/react": "17.0.2",
		"@types/react-bootstrap": "^0.32.32",
		"@types/react-dom": "17.0.2",
		"@types/styled-components": "5.1.10",
		"@typescript-eslint/eslint-plugin": "4.28.1",
		"@typescript-eslint/parser": "4.28.1",
		"autoprefixer": "10.4.12",
		"babel-loader": "8.2.5",
		"bootstrap": "3.4.1",
		"chromatic": "6.10.4",
		"copyfiles": "2.4.1",
		"css-loader": "3.2.0",
		"enzyme": "3.11.0",
		"enzyme-adapter-react-17-updated": "1.0.2",
		"enzyme-to-json": "3.6.2",
		"eslint": "7.29.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-import-resolver-typescript": "3.5.0",
		"eslint-import-resolver-webpack": "0.13.1",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jest": "27.1.4",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-react": "7.31.8",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-storybook": "0.6.4",
